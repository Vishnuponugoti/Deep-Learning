   "source": [
    "# Taking input from user\n",
    "string = list(\"Python\")\n",
    "# Delete 'n' and 'y' characters\n",
    "string = [x for x in string if x not in ('n','y')]\n",
    "# Reverse the resultant string\n",
    "string = string[::-1]\n",
    "# Print the reversed string\n",
    "print(\"\".join(string))"
   ]
  }
